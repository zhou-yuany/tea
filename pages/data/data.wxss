/* pages/data/data.wxss */
.headtop{
  width: 100%;
  height: 70rpx;
  margin: auto;
  display: flex;
  justify-content: flex-end;
  align-items: center;
}
.headtop image{
  width: 25rpx;
  height: 26rpx;
  display: block;
  margin-right: 23rpx;
}
/* 列表 */
.dataList{}
.listitem{
  width: 665rpx;
  height: 117rpx;
  margin: auto;
  border-bottom: 1px solid #EEEEEE;
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.item_left{
  width: 50%;
  display: flex;
  justify-content: flex-start;
  align-items: center;
}
.item_left image{
  width: 32rpx;
  height: 32rpx;
  display: block;
  margin-right: 30rpx;
}
.item_left text{
  font-size: 30rpx;
}
.arrow{
  width: 12rpx;
  height: 22rpx;
  display: block;
}