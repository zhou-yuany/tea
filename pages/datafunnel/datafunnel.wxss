.modek{
  width: 100%;
  height: 70rpx;
  margin: auto;
  display: flex;
  justify-content: flex-end;
  align-items: center;
}
.modek image{
  width: 26rpx;
  height: 24rpx;
  display: block;
  margin-right: 22rpx;
}
.funnel{
  width: 100%;
  height: auto;
  margin: auto;
  padding: 35rpx 0;
  display: flex;
  justify-content: space-between;
  align-items: center;
}
