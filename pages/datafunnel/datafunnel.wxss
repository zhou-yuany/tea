  width: 310rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
  margin-left: 27rpx;
}
.oricl{
  border-top: 95rpx solid #FB781D;
  border-left: 20rpx solid transparent;
  border-right: 20rpx solid transparent; 
  height: 0; 
  width: 266rpx;
  position: relative;
}
