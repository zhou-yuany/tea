/* pages/ranking/ranking.wxss */
.modek{
  width: 100%;
  height: 70rpx;
  margin: auto;
  display: flex;
  justify-content: flex-end;
  align-items: center;
}
.modek image{
  width: 26rpx;
  height: 24rpx;
  display: block;
  margin-right: 22rpx;
}
.tr {
  display: flex;
  width: 100%;
  justify-content: flex-start;
  height: 115rpx;
  align-items: center;
  border-bottom: 1px solid #EEEEEE;
}
.td {
  width:25%;
  justify-content: center;
  text-align: center;
}
.th {
  width: 25%;
  justify-content: center;
  display: flex;
  height: 115rpx;
  align-items: center;
}