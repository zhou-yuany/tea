@import "/app.wxss";

.box{
  padding-top: 10rpx;
}
.swiper-tab{
  width: 100%;
  text-align: center;
  line-height: 80rpx;
}
.swiper-tab-list{  
  font-size: 32rpx;
  display: inline-block;
  width: 25%;
  color: #333333;
}
.line{
  width: 64rpx;
  height: 3rpx;
  margin: auto;
}
.on{ 
  color: #EE7500;
}
.tab-scoller {
  width: 100%;
  box-sizing: border-box;
}
.swiper-box{ 
  display: block; 
  height: 100%; 
  width: 100%; 
  padding-top: 21rpx;
}
.group{
  width: 650rpx;
  height: auto;
  margin: auto;
  box-shadow: 0rpx 7rpx 7rpx 1rpx rgba(0, 0, 0, 0.04);
  border-radius: 10rpx;
  padding: 0 30rpx;
  margin-bottom: 20rpx;
}
.head{
  width: 100%;
  height: 83rpx;
  margin: auto;
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.head view{
  display: flex;
  justify-content: flex-start;
  align-items: center;
}
.head view image{
  width: 20rpx;
  height: 33rpx;
  margin-right: 19rpx;
}
.head view text{
  font-size: 30rpx;
}
.state{
  font-size: 28rpx;
}
.middle{
  width: 100%;
  height: auto;
  margin: auto;
}
.addressk{
  width: 100%;
  margin: auto;
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding: 31rpx 0;
}
.left{
  width: 75%;
}
.startend{
  display: flex;
  justify-content: flex-start;
  align-items: center;
}
.startend view{
  width: 33rpx;
  height: 33rpx;
  border-radius: 50%;
  font-size: 20rpx;
  text-align: center;
  line-height: 33rpx;
  margin-right: 10rpx;
}
.startend text{
  font-size: 34rpx;
  line-height: 51rpx;
}
.right{
  display: flex;
  flex-direction: column;
  align-items: flex-end;
}
.right text{
  font-size: 32rpx;
}
.number{
  font-size: 24rpx;
  line-height: 40rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.foot{
  width: 100%;
  height: 98rpx;
  margin: auto;
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.foot text{
  font-size: 24rpx;
}
.foot button{
  width: 180rpx;
  height: 68rpx;
  border-radius: 34rpx;
  font-size: 28rpx;
  text-align: center;
  line-height: 68rpx;
  margin: 0;
}